                            <tr class="prop">
                                <td valign="top" class="name">
                                    <label for="warranty">Warranty</label>
                                </td>
                                <td valign="top" class="value ${hasErrors(bean: itemMasterExtInstance, field: 'residentialWarrantyCode', 'errors')}">
                                   <g:select name="residentialWarrantyCode"   
                                        from="${WarrantyPeriod.list()}" optionKey="id"
                                        noSelection="['':'-Set warranty period-']"
                                    />                                
                                </td>
                            </tr>
                        
                            <tr class="prop">
                                <td valign="top" class="name">
                                    <label for="weightLimit"><g:message code="itemMasterExt.weightLimit.label" default="Weight Limit" /></label>
                                </td>
                                <td valign="top" class="value ${hasErrors(bean: itemMasterExtInstance, field: 'weightLimit', 'errors')}">
                                    <g:textField name="weightLimit" value="${fieldValue(bean: itemMasterExtInstance, field: 'weightLimit')}" />
                                </td>
                            </tr>
                        
                        </tbody>
                    </table>
                </div>
                <div class="buttons">
                    <span class="button"><g:submitButton name="create" class="save" value="${message(code: 'default.button.create.label', default: 'Create')}" /></span>
                </div>
            </g:form>
        </div>
    </div>
    </body>
</html>
