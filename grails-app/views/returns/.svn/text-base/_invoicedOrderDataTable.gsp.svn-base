<gui:dataTable
         caption="Click row to select"
         draggableColumns="true"
         id="invoicedOrderDataTable"
         columnDefs="[
             [key:'orderNo', sortable:true, resizeable: true, label:'Number'],
             [key:'poNo', sortable:true, resizeable: true, label:'PO No.'],
             [key:'orderDate', sortable:true, resizeable: true, label:'Order Date']
		 ]"
         controller="returns" action="invoicedOrderDataTableAsJSON" params="[custNo:params.custNo]"
         rowClickNavigation="false"
         collapseOnExpansionClick="true"
         rowsPerPage="12"
         sortedBy='orderNo'
 />
